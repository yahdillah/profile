{
    "version": 2,
    "framework": null,
    "functions": {
        "api/**/*.js": {
          "runtime": "@vercel/node@2.0.0"
        },
        "api/**/*.py": {
          "runtime": "@vercel/python@3.9.0"
        }
    },
    "routes": [
        {
            "src": "/(build|assets|css|js|images|storage|favicon.ico)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://${VERCEL_URL}",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "SESSION_DRIVER": "array",
        "LOG_CHANNEL": "stderr"
    }
}
